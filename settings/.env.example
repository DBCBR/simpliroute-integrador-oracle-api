## Exemplo de variáveis de ambiente para integração SimpliRoute / Gnexum
# NÃO coloque tokens reais neste arquivo. Use apenas como referência.

# Token para SimpliRoute
SIMPLIR_ROUTE_TOKEN=

# Token/Chave para Gnexum
GNEXUM_TOKEN=

# Porta usada pelo webhook local
WEBHOOK_PORT=8000

# URL base caso queira sobrescrever a do config.yaml
SIMPLIR_ROUTE_BASE_URL=
